#. [scenario]: id=2_04_World
#. [scenario]: id=2_03_World
#. [scenario]: id=2_05_World
#. [scenario]: id=1_06_World
#. [scenario]: id=1_08_World
#. [scenario]: id=1_04_World
#. [scenario]: id=1_09_World
#. [scenario]: id=1_07_World
#. [scenario]: id=1_10_World
#. [scenario]: id=1_02_World
#: Bad_Moon_Rising/scenarios3/2_02_World.cfg:4
#: Bad_Moon_Rising/scenarios3/2_04_World.cfg:4
#: Bad_Moon_Rising/scenarios3/2_03_World.cfg:4
#: Bad_Moon_Rising/scenarios3/2_05_World.cfg:4
#: Bad_Moon_Rising/scenarios2/1_06_World.cfg:4
#: Bad_Moon_Rising/scenarios2/1_08_World.cfg:4
#: Bad_Moon_Rising/scenarios2/1_04_World.cfg:4
