#: Bad_Moon_Rising/scenarios2/1_08_Rumble.cfg:22
#: Bad_Moon_Rising/scenarios2/1_07_Crush.cfg:45
msgid "Defeat Enemy Leaders"
msgstr ""

#. [objective]: condition=lose
#: Bad_Moon_Rising/scenarios3/2_04_Pond.cfg:28
#: Bad_Moon_Rising/scenarios3/2_06_Gate.cfg:38
#: Bad_Moon_Rising/scenarios3/2_06_Gate.cfg:499
#: Bad_Moon_Rising/scenarios3/2_06_Gate.cfg:514
#: Bad_Moon_Rising/scenarios3/2_01_Trouble.cfg:34
#: Bad_Moon_Rising/scenarios3/2_04_Checkpoint.cfg:31
#: Bad_Moon_Rising/scenarios3/2_04_Checkpoint.cfg:322
#: Bad_Moon_Rising/scenarios3/2_05_Shelter.cfg:27
#: Bad_Moon_Rising/scenarios3/2_07_Dreams1.cfg:987
#: Bad_Moon_Rising/scenarios3/2_04_River.cfg:38
#: Bad_Moon_Rising/scenarios3/2_03_Fundraising.cfg:34
#: Bad_Moon_Rising/scenarios3/2_05_Tien.cfg:33
#: Bad_Moon_Rising/scenarios3/2_05_Tien.cfg:504
#: Bad_Moon_Rising/scenarios3/2_02_MineMouth.cfg:35
