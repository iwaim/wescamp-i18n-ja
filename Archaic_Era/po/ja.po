#: Archaic_Era/utils/Archaic_abilities.cfg:12
msgid "Immune to drain, poison and plague"
msgstr "ドレイン、毒、疫病に免疫を持ちます"

#. [trait]: id=aa_zealous
#: Archaic_Era/utils/Archaic_abilities.cfg:24
msgid "zealous"
msgstr "熱狂"

#. [trait]: id=aa_zealous
#: Archaic_Era/utils/Archaic_abilities.cfg:25
